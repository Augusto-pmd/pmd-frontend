    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3"
  },
  "overrides": {
    "glob": "^10.5.0"
  }
}
