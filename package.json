{
  "name": "pmd-frontend",
  "version": "1.0.1",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "node -e \"const fs=require('fs');const path=require('path');const rimraf=require('rimraf');['.next','node_modules/.cache','.next/cache'].forEach(d=>{const p=path.join(__dirname,d);if(fs.existsSync(p)){try{fs.rmSync(p,{recursive:true,force:true,maxRetries:3,retryDelay:100});console.log('Cleaned:',d);}catch(e){console.warn('Warning: Could not clean',d,e.message);}}});\""
  },
  "dependencies": {
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "gantt-task-react": "^3.0.0",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.555.0",
    "next": "^14.2.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^2.4.0",
    "xlsx": "^0.18.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
